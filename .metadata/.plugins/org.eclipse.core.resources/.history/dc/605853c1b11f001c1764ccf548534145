package summ;

import java.util.Scanner;

public class Sum {
public static void main(String arg[])
{
	Scanner readme=new Scanner(System.in);
	System.out.println("Enter the Digits:" );
	int i;
	i=readme.nextInt();
	System.out.println("Sum of Digits:");
	for(i=0;i<100;i=i+1)
	{
		System.out.println(i);
	}
}
}
